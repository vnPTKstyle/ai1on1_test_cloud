{
  "name": "testCloudRun",
  "version": "1.0.0",
  "private": true,
  "description": "NestJS 10 + TypeORM + MySQL + Passport JWT/Local",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "migration:run": "nest build && node node_modules/typeorm/cli.js migration:run -d dist/data-source.js",
    "migration:revert": "node node_modules/typeorm/cli.js migration:revert -d dist/data-source.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/typeorm": "^10.0.2",
    "@aws-sdk/client-comprehend": "^3.823.0",
    "@aws-sdk/client-s3": "^3.911.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mysql2": "^3.11.3",
    "openai": "^5.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "@nestjs/cli": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "typescript": "~5.1.3"
  }
}
